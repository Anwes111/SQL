WITH NEW AS (
SELECT 'HS7B5TK.11' AS SLASH_LOT_ID FROM DUAL UNION ALL
SELECT 'HSSK5TK.11' AS SLASH_LOT_ID FROM DUAL)
SELECT CONCAT(SUBSTRING(SLASH_LOT_ID,1,3),SUBSTRING(SLASH_LOT_ID,5,2),SUBSTRING(SLASH_LOT_ID,8,2),'.',
                              SUBSTRING(SLASH_LOT_ID,11,3)) AS LOT_ID FROM NEW;

WITH NEW AS (
SELECT 1 AS COL1 FROM DUAL UNION ALL
SELECT 2 AS COL1 FROM DUAL UNION ALL
SELECT 4542 AS COL1 FROM DUAL UNION ALL
SELECT 342 AS COL1 FROM DUAL)
SELECT SUM(COL1) QTY FROM NEW;
